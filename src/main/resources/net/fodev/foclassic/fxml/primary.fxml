<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" stylesheets="@../css/primary.css" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.fodev.foclassic.controller.PrimaryController">
   <children>
      <Pane fx:id="backgroundPane" maxWidth="1024.0" prefHeight="768.0" prefWidth="1024.0">
         <children>
            <Label fx:id="labelNew" layoutX="70.0" layoutY="45.0" prefHeight="30.0" prefWidth="140.0" styleClass="textTall" text="NEW CHAR" />
            <Label fx:id="labelLoad" layoutX="70.0" layoutY="97.0" prefHeight="30.0" prefWidth="140.0" styleClass="textTall" text="LOAD CHAR" />
            <Label fx:id="labelExit" layoutX="70.0" layoutY="148.0" prefHeight="30.0" prefWidth="140.0" styleClass="textTall" text="EXIT" />

            <Button fx:id="buttonNew" layoutX="47.0" layoutY="49.0" onAction="#switchToRegister" text="" styleClass="buttonRedCircleMenu" />
            <Button fx:id="buttonLoad" layoutX="47.0" layoutY="101.0" onAction="#switchToSecondary" text="" styleClass="buttonRedCircleMenu" />
            <Button fx:id="buttonExit" layoutX="47.0" layoutY="153.0" onAction="#switchToExit" text="" styleClass="buttonRedCircleMenu" />
         </children>
      </Pane>
   </children>
</VBox>
